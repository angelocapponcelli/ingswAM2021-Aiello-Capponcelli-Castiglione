<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="526.0" prefWidth="884.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.editor.VaticanReportSectionEditController">
   <children>
      <Pane prefHeight="405.0" prefWidth="884.0">
         <children>
            <TextField fx:id="points" layoutX="534.0" layoutY="250.0" />
            <Label layoutX="472.0" layoutY="125.0" prefHeight="66.0" prefWidth="272.0" text="Victory Points" textAlignment="CENTER">
               <font>
                  <Font size="44.0" />
               </font>
            </Label>
            <TextField fx:id="number" layoutX="103.0" layoutY="250.0" />
            <Label layoutX="30.0" layoutY="125.0" prefHeight="66.0" prefWidth="334.0" text="Section Number" textAlignment="CENTER">
               <font>
                  <Font size="44.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <ButtonBar buttonMinWidth="200.0" prefHeight="115.0" prefWidth="884.0">
        <buttons>
            <Button fx:id="discard" mnemonicParsing="false" onAction="#discard" style="-fx-background-color: red;" text="Discard" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
            <Button fx:id="okButton" mnemonicParsing="false" onAction="#insertedVictoryPoints" style="-fx-background-color: GREEN;" text="OK" textFill="WHITE">
               <font>
                  <Font size="24.0" />
               </font>
            </Button>
        </buttons>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <padding>
            <Insets right="30.0" />
         </padding>
      </ButtonBar>
   </children>
</VBox>
